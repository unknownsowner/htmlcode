<!DOCTYPE html>
<html lang="en-us">
<head>
  <!-- Original head content -->
  <meta charset="utf-8">
  <meta name="robots" content="noindex, nofollow">
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <title>google.com</title>
  <link href="https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/TemplateData/style.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/TemplateData/UnityProgress.js" type="text/javascript"></script>

  <!-- ✅ Custom Loading Screen Styles -->
  <link rel="shortcut icon" href="https://cdn.jsdelivr.net/gh/unknownsowner/imagebackground/UnknownsFavicon.png" />
  <style>
    #custom-loading-screen {
      position: fixed;
      top: 0; left: 0;
      width: 100%; height: 100%;
      background: url('https://cdn.jsdelivr.net/gh/unknownsowner/imagebackground@main/Untitled%20design.png') no-repeat center center;
      background-size: cover;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      z-index: 99999;
    }
    #custom-loading-screen img {
      width: 150px;
      height: 150px;
      margin-bottom: 20px;
    }
    #custom-loading-bar {
      width: 300px;
      height: 20px;
      background-color: #333;
      border-radius: 10px;
      overflow: hidden;
    }
    #custom-loading-progress {
      height: 100%;
      width: 0%;
      background-color: #00ffcc;
      transition: width 7s linear;
    }
  </style>
  <script>
    function startCustomLoading() {
      document.getElementById("custom-loading-progress").style.width = "100%";
      setTimeout(function() {
        const loader = document.getElementById("custom-loading-screen");
        if (loader) loader.style.display = "none";
      }, 7000);
    }
  </script>
</head>

<body class="template" onload="startCustomLoading();">
  <!-- ✅ Custom Loading Screen -->
  <div id="custom-loading-screen">
    <img src="https://cdn.jsdelivr.net/gh/unknownsowner/imagebackground/UnknownsFavicon.png" alt="Loading Icon" />
    <div id="custom-loading-bar">
      <div id="custom-loading-progress"></div>
    </div>
  </div>

  <!-- ✅ Original Unity Canvas -->
  <div class="template-wrap clear">
    <canvas class="emscripten" id="canvas" oncontextmenu="event.preventDefault()" height="528" width="813"></canvas>
  </div>

  <script type="text/javascript">
    var Module = {
      TOTAL_MEMORY: 268435456,
      errorhandler: null,
      compatibilitycheck: null,
      backgroundColor: "#222C36",
      splashStyle: "Dark",
      dataUrl: "https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/Release/NG.data",
      codeUrl: "https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/Release/NG.js",
      asmUrl: "https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/Release/NG.asm.js",
      memUrl: "https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/Release/NG.mem",
    };
  </script>
  <script src="https://cdn.jsdelivr.net/gh/classrooms-1/google.com@main/Release/UnityLoader.js"></script>

  <!-- ✅ Original Gadget/Module Wrapping -->
  <module>
    <moduleprefs title="Google.com">
      <content type="html"><![CDATA[
        <!-- Your Unity game is embedded here -->
      ]]></content>
    </moduleprefs>
  </module>

  <!-- ✅ Original Embedded Scripts -->
